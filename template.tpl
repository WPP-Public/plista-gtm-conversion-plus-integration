// Enter your template code here.
const log = require('logToConsole');
const getQueryParameters = require('getQueryParameters');
const setCookie = require('setCookie');

log("starting with input data:");
log(data);

const token = getQueryParameters("token");

if (token) {
  log("found token "+token+" in url, setting cookie");
  
  const options = {
    domain: 'auto',
    path: '/',
    secure: true,
    httpOnly: false,
    "max-age": 1 * 365 * 24 * 60 * 60,
  };
  
  log("setting cookie with options:");
  log(options);
  
  // todo: support multiple tokens being set, for multiple camps running on the same advertiser
  setCookie("plt", token, options, false);
}
else {
  log("did not find a token in url");
}

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


